package s6.ReceipeFood.modele;

import java.util.List;
import java.util.Vector;

public class Composant extends BaseModel{
	String nom;
	List<Produit> ltProduit = new Vector<Produit>();
	Unite unite;
	
	public Composant() {}

	public Composant(int id, String nom) {
		super(id);
		this.setNom(nom);
	}

	public String getNom() {
		return nom;
	}

	public void setNom(String nom) {
		this.nom = nom;
	}

	public List<Produit> getLtProduit() {
		return ltProduit;
	}

	public void setLtProduit(List<Produit> ltProduit) {
		this.ltProduit = ltProduit;
	}

	public Unite getUnite() {
		return unite;
	}

	public void setUnite(Unite unite) {
		this.unite = unite;
	}
	
	
}
