package s6.ReceipeFood.modele;

public class Composant extends BaseModel{
	String nom;
	Produit produit;
	
	public Composant() {}

	public Composant(int id, String nom) {
		super(id);
		this.setNom(nom);
	}

	public String getNom() {
		return nom;
	}

	public void setNom(String nom) {
		this.nom = nom;
	}
	
	
}
