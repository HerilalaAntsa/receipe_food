<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="s6.ReceipeFood.modele.Administrateur" table="administrateur">
        <meta attribute="class-description">
            This class contains the administrateur detail.
        </meta>
        <id name="id" type="int" column="idadmin">
            <generator class="sequence">
            	<param name="sequence_name">administrateur_idadministrateur_seq</param>
            </generator>
        </id>
        <property name="nom" column="nomadmin" type="string"/>
        <property name="password" column="password" type="string"/>
    </class>
</hibernate-mapping>